server{
  listen 80;

  path1{
    location /echo;
    handler EchoHandler;
  }

  path2{
    location /static;
    root ./static;
    handler StaticHandler;
  }

  path3{
    location /static2;
    root ./www/data;
    handler StaticHandler;
  }

  path4{
    location /;
    handler NotFoundHandler;
  }
}
